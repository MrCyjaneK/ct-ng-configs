            "*:{*-linux-gnu,*-linux-musl,*-w64-mingw32}:cp -a $HOME/x-tools/$TOOL_TARGET/*/sysroot/sbin $STAGING_DIR$PREFIX/",
            "*:{*-linux-gnu,*-linux-musl,*-w64-mingw32}:cp -a $HOME/x-tools/$TOOL_TARGET/*/sysroot/usr $STAGING_DIR$PREFIX/",
            "*:{*-linux-gnu,*-linux-musl,*-w64-mingw32}:mkdir -p $STAGING_DIR$PREFIX/include",
